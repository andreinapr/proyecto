{% extends 'SedaBundle::layout.html.twig' %}

{% block contenido %}
{#
  {{ formulario | ld }}  
#}
		{{ form_start(formulario, { 'attr': {'novalidate': 'novalidate', 'class': 'form-inline', 'id': 'formulario'} }) }}
		    {{ form_errors(formulario) }}

	    <div class="row" align= "center">
	   	    <div class="form-group">
		    	<div>{{ form_label(formulario.evaluacion, 'Estado de Evaluacion') }}</div>	
		        {{ form_widget(formulario.evaluacion, { 'attr': {'class': 'form-control'} }) }}
			    <div class="alert-danger" align= "center">{{ form_errors(formulario.evaluacion) }}</div>        
		    </div>
	    </div>
		<br>
	    <div class="row" align= "center">
			<input class="btn btn-primary" type="submit" value="Guardar" />
		</div>
		{{ form_end(formulario) }}

{% endblock %}
{% block javascripts %}
	{{ parent() }}
	<script type="text/javascript" src="{{ asset('js/tablas.js') }}"></script>
{% endblock %}