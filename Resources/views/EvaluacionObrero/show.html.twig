                <div class="alert-danger" align= "center">{{ form_errors(formulario.observacionOGTH) }}</div>
            </div>  
        </div>


        <br>
        <div class="row" align="center">
            <input class="btn btn-primary" type="submit" value="Guardar" />&nbsp;&nbsp;
        </div>


    {{ form_end(formulario) }}      

